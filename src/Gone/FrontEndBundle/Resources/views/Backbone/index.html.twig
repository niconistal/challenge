<html>
	<head>
		{% stylesheets '@GoneFrontEndBundle/Resources/public/css/bootstrap.css'
                        '@GoneFrontEndBundle/Resources/public/css/main.css'
                        filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
	</head>
    <body>
        <div class="jumbotron header">
            {% image '@GoneFrontEndBundle/Resources/public/images/logo.png' %}
                <img src="{{ asset_url }}" alt="Example" />
            {% endimage %}
            <h1>Gone! - Challenge Project</h1>
        </div>
        <div id="main">Loading...</div>

        {% javascripts
            '@GoneFrontEndBundle/Resources/public/js/libs/jquery-min.js'
            '@GoneFrontEndBundle/Resources/public/js/libs/snippets.js'
            '@GoneFrontEndBundle/Resources/public/js/libs/bootstrap-min.js'
            '@GoneFrontEndBundle/Resources/public/js/libs/underscore-min.js'
            '@GoneFrontEndBundle/Resources/public/js/libs/handlebars.js'
            '@GoneFrontEndBundle/Resources/public/js/libs/backbone-min.js'
            '@backbone_app' %}
                <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    </body>
</html>
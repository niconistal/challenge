{% block content %}
    <h1>Create Box Form</h1>
    {%  if (form is not null) %}

    <form action="{{ url('api_post_box') }}" method="POST" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <input type="submit" value="submit">
    </form>
    {{form_end(form)}}
    {% endif %}
{% endblock %}
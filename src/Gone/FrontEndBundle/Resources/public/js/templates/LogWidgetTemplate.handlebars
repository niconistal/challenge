<h4>Events History</h4>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Date</th>
            <th>Event</th>
        </tr>
    </thead>
    <tbody>
        {{#each log}}
            <tr>
                <td>{{date}}</td>
                <td>{{detail}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>